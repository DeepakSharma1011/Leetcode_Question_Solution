class Solution {
    public void moveZeroes(int[] nums) {
     int n = nums.length;
     int NonZeroIndex = 0;
     for(int i = 0; i<n; i++){
        if(nums[i] != 0){
            nums[NonZeroIndex] = nums[i];
            NonZeroIndex++;
        }
     }
     for(int i = NonZeroIndex ; i <n ; i++){
        nums[i] = 0;
     }
    }
}
