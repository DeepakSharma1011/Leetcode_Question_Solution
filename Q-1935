class Solution {
    public int canBeTypedWords(String text, String brokenLetters) {
        Set<Character> set = new HashSet<>();

        for(char c : brokenLetters.toCharArray()){
            set.add(c);
        }

        String w[] = text.split(" ");
        int count = 0;
        for(String ws : w){
            boolean ok = true;
            for(char c : ws.toCharArray()){
                if(set.contains(c)){
                    ok = false;
                    break;
                }
            }
            if(ok == true){
                count++;
            }
        }
        return count;
    }
}
