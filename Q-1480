class Solution {
    public int[] runningSum(int[] nums) {
        int n = nums.length;
        if(n == 1) return nums;
        int presum[] = new int[n];
        presum[0] = nums[0];
        for(int i = 1; i<n; i++){
            presum[i] = nums[i]+presum[i-1];
        }
        return presum;
    }
}
