class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int n = nums.length;
        int maxn = 0;
        int count = 0;
        for(int i = 0; i<n; i++){
            int max = maxn;
            if(nums[i] == 1){
                count++;
            }
            else{
                count = 0;
            }
            maxn = Math.max(max,count);
        }
        return maxn;
    }
}
